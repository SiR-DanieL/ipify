{
  "manifest_version": 2,

  "name": "ipify",
  "description": "This extension shows your current public IP address in an awesome popup.",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "http://api.ipify.org/"
  ]
}
